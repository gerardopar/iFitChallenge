{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": ["airbnb", "plugin:jest/recommended"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "jest"
    ],
    "rules": {
        "indent": "off",
        "no-console": "off",
        "no-unused-vars": "off",
        "react/jsx-filename-extension": "off",
        "react/jsx-fragments": "off",
        "react/jsx-indent": "off",
        "react/jsx-props-no-spreading": "off",
        "react/prop-types": "off"
    }
}